{
  "manifest_version": 3,
  "name": "AI Jokes & Quotes Screensaver",
  "version": "1.0",
  "description": "A beautiful screensaver that displays AI-related jokes and inspiring quotes",
  "permissions": ["idle"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
} 